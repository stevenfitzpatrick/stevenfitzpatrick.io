{
	"name": "stevenfitzpatrick.io",
	"version": "1.0.0",
	"description": "Homepage of Steven Fitzpatrick",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --colors --watch",
		"test": "npm run lint && cross-env NODE_ENV=test npm run -s test:karma",
		"test:chrome": "eslint cross-env NODE_ENV=test src/**/*.js && npm run -s test:karma -- --browsers ChromeCanaryHeadless",
		"test:karma": "karma start test/karma.config.js --single-run",
		"build": "cross-env NODE_ENV=production BABEL_ENV=production webpack -p --display-optimization-bailout",
		"publish": "npm run test && npm run build && aws s3 rm s3://stevenfitzpatrick.io/ --recursive && aws s3 cp ./dist s3://stevenfitzpatrick.io/ --recursive --metadata-directive REPLACE --cache-control \"private, no-cache, max-age=0\" --exclude * --include sw.js --include index.html && aws s3 cp ./dist s3://stevenfitzpatrick.io/ --recursive --metadata-directive REPLACE --cache-control public, max-age=31536000 --exclude sw.js --exclude index.html",
		"lint": "eslint ./src ./test",
		"lint:fix": "npm run lint -- --fix"
	},
	"browserslist": [
		"> 5%",
		"last 1 version"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stevenfitzpatrick/stevenfitzpatrick.io.git"
	},
	"keywords": [
		"Front",
		"End",
		"Developer",
		"Steven",
		"Fitzpatrick"
	],
	"author": "Steven Fitzpatrick",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/stevenfitzpatrick/stevenfitzpatrick.io/issues"
	},
	"homepage": "stevenfitzpatrick.io",
