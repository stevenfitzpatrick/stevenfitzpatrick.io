{
	"name": "stevenfitzpatrick.io",
	"version": "1.0.0",
	"description": "Homepage of Steven Fitzpatrick",
	"main": "index.js",
	"scripts": {
		"build": "cross-env NODE_ENV=production BABEL_ENV=production webpack -p",
		"deploy": "aws s3 rm s3://stevenfitzpatrick.io/ --recursive && aws s3 cp ./dist s3://stevenfitzpatrick.io/ --recursive --metadata-directive REPLACE --cache-control private,no-cache,max-age=0 --exclude \"*\" --include sw.js --include index.html && aws s3 cp ./dist s3://stevenfitzpatrick.io/ --recursive --metadata-directive REPLACE --cache-control public,max-age=31536000 --exclude sw.js --exclude index.html",
		"lint": "eslint ./src ./test",
		"lint:fix": "npm run lint -- --fix",
		"minify": "minify ./dist/sw.js --out-file ./dist/sw.js",
		"predeploy": "npm run test && npm run build && npm run minify",
		"start": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --colors --watch",
		"test": "npm run lint && npm run test:size",
		"test:size": "bundlesize"
	},
	"browserslist": [
		"last 2 Chrome versions"
	],
	"bundlesize": [
		{
			"path": "./dist/vendor.bundle.*.js",
			"maxSize": "30 kB"
    },
    {
			"path": "./dist/app.bundle.*.js",
			"maxSize": "20 kB"
		},
    {
			"path": "./dist/index.html",
			"maxSize": "5 kB"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stevenfitzpatrick/stevenfitzpatrick.io.git"
	},
	"keywords": [
		"Front",
		"End",
		"Developer",
		"Steven",
		"Fitzpatrick"
	],
	"eslintIgnore": [
		"dist/*"
	],
	"author": "Steven Fitzpatrick",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/stevenfitzpatrick/stevenfitzpatrick.io/issues"
	},
	"homepage": "https://stevenfitzpatrick.io",
	"devDependencies": {
		"autoprefixer": "^7.1.6",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.0.1",
		"babel-loader": "^7.1.2",
		"babel-minify": "^0.2.0",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.10",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.26.0",
		"babel-runtime": "^6.22.0",
		"bundlesize": "^0.15.3",
		"chunk-manifest-webpack-plugin": "^1.1.2",
		"clean-webpack-plugin": "^0.1.17",
		"copy-webpack-plugin": "^4.2.0",
		"core-js": "^2.5.1",
		"cross-env": "^5.1.0",
		"css-loader": "^0.28.7",
		"eslint": "4.9.0",
		"eslint-config-prettier": "^2.6.0",
		"eslint-config-synacor": "^2.0.2",
		"eslint-loader": "1.9.0",
		"eslint-plugin-import": "2.7.0",
		"eslint-plugin-jsx-a11y": "^6.0.2",
		"eslint-plugin-prettier": "^2.3.1",
		"eslint-plugin-react": "^7.4.0",
		"extract-text-webpack-plugin": "^3.0.1",
		"file-loader": "^1.1.5",
		"html-loader": "^0.5.1",
		"html-webpack-plugin": "^2.30.1",
		"image-webpack-loader": "^3.4.2",
		"inline-manifest-webpack-plugin": "^3.0.1",
		"modularscale-sass": "^3.0.3",
		"node-sass": "^4.5.3",
		"postcss-import": "^11.0.0",
		"postcss-loader": "^2.0.8",
		"postcss-url": "^7.1.2",
		"preact-redux": "^2.0.3",
		"preload-webpack-plugin": "^2.0.0",
		"resource-hints-webpack-plugin": "0.0.1",
		"sass-loader": "^6.0.6",
		"script-ext-html-webpack-plugin": "^1.8.7",
		"style-ext-html-webpack-plugin": "^3.4.3",
		"style-loader": "^0.19.0",
		"stylelint-config-standard": "^17.0.0",
		"svg-sprite-loader": "^3.4.1",
		"url-loader": "^0.6.2",
		"webpack": "3.8.1",
		"webpack-bundle-analyzer": "^2.9.0",
		"webpack-dev-server": "^2.9.3",
		"workbox-webpack-plugin": "^2.1.1"
	},
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"classnames": "^2.2.5",
		"highlight.js": "^9.12.0",
		"preact": "^8.2.6",
		"preact-async-route": "^2.0.1",
		"preact-markup": "^1.6.0",
		"preact-router": "^2.6.0",
		"redux": "^3.7.2",
		"redux-saga": "^0.16.0",
		"styled-components": "^2.2.2",
		"unfetch": "^3.0.0"
	}
}
