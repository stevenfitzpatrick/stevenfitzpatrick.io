{
  "name": "stevenfitzpatrick.io",
  "version": "1.0.0",
  "description": "Homepage of Steven Fitzpatrick",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --colors --watch",
    "test": "eslint cross-env NODE_ENV=test src/**/*.js && npm run -s test:karma",
    "test:chrome": "eslint cross-env NODE_ENV=test src/**/*.js && npm run -s test:karma -- --browsers ChromeCanaryHeadless",
    "test:karma": "karma start test/karma.config.js --single-run",
    "build": "cross-env NODE_ENV=production webpack -p --display-optimization-bailout",
    "publish": "npm run test && npm run build && aws s3 rm s3://stevenfitzpatrick.io/ --recursive && aws s3 cp ./dist s3://stevenfitzpatrick.io/ --recursive --metadata-directive REPLACE --cache-control \"private, max-age=0\" --exclude * --include sw.js --include index.html && aws s3 cp ./dist s3://stevenfitzpatrick.io/ --recursive --metadata-directive REPLACE --cache-control max-age=31536000 --exclude sw.js --exclude index.html"
  },
  "browserslist": ["> 5%", "last 1 version"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenfitzpatrick/stevenfitzpatrick.io.git"
  },
